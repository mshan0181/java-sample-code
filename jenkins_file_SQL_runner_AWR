node ('master') {
   stage('Code_checkout') {
   git url: 'https://github.com/mshan0181/java-sample-code.git'
   }
   stage('SQL_script_runner_AWR_report') { 
   echo 'SQLPlusRunner_AWR_report running user define script for shandba@client_access'
   bat '(echo define DB_ID\='%dbid%'" & type %AWR_report.sql%) > temp.txt'
   bat ' move /y temp.txt %AWR_report.sql%'
   bat '(echo "define INSTANCE_NO='%inst_num%'" & type %AWR_report.sql%) > temp.txt'
   bat ' move /y temp.txt %AWR_report.sql%'
   bat '(echo "define BEGIN_ID='%bid%'" & type %AWR_report.sql%) > temp.txt'
   bat ' move /y temp.txt %AWR_report.sql%'
   bat '(echo "define BEGIN_ID='%eid%'" & type %AWR_report.sql%) > temp.txt'
   bat ' move /y temp.txt %AWR_report.sql%'
   step([$class: 'SQLPlusRunnerBuilder',credentialsId:'shandb-priv-id', instance:'client_access',scriptType:'file', script: 'AWR_report.sql',scriptContent: ''])
   bat 'type awr_from_console.htm'
}   
}
