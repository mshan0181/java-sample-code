node ('master') {
   stage('Code_checkout') {
   git url: 'https://github.com/mshan0181/java-sample-code.git'
   }
   stage('SQL_script_runner_AWR_report') {    
    bat 'echo "define DB_ID=%dbid%" >> AWS_SQL_temp.sql' 
    bat 'echo "define INSTANCE_NO=%inst_num%" >> AWS_SQL_temp.sql'
    bat 'echo "define BEGIN_ID=%bid%" >> AWS_SQL_temp.sql'
    bat 'echo "define END_ID=%eid%" >> AWS_SQL_temp.sql'
    bat 'type AWS_SQL_temp.sql'
       }   
}
