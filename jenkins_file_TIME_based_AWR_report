         withEnv(["dbid_var=2940744064"])  {
            echo env.dbidvar 
            bat 'echo define DB_ID=%dbid_var% > AWR_SQL_temp.sql'      
                                                }
            withEnv(["instnum_var=1"]) {
               echo env.instnum_var  
